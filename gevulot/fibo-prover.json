{
    "RebootOnExit": true,
    "RunConfig": {
      "CPUs": 1,
      "Memory": "2g"
    },
    "Env":{
      "GLOG_logtostderr": "1"
    },
    "Dirs":["gevulot"],
    "Args": [
    "--out_file=/deployments/fibo_proof.json",
    "--private_input_file=/gevulot/fibonacci_private_input.json",
    "--public_input_file=/gevulot/fibonacci_public_input.json",
    "--prover_config_file=/gevulot/cpu_air_prover_config.json",
    "--parameter_file=/gevulot/cpu_air_params.json" ],
    "Program":"cpu_air_prover"
  }
  